<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lyRep_iii"
    android:layout_width="match_parent" android:layout_height="match_parent"
    >

    <ImageButton
        android:id="@+id/ibSelecCarga_rep_ii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/mancuerna"
        tools:style="@android:style/Widget.Holo.Light.ImageButton"
        android:layout_alignBottom="@+id/txtCarga_rep_ii_1"
        android:layout_toRightOf="@+id/textView30"
        android:layout_toEndOf="@+id/textView30" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Prueba de Repetibilidad"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        android:textStyle="bold"
        android:typeface="serif"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btGuardar_rep_ii"
        android:textSize="12sp"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardar"
        android:layout_below="@+id/txtemp_rep_ii"
        android:layout_toRightOf="@+id/lbUniRepiii"
        android:layout_toEndOf="@+id/lbUniRepiii" />

    <TextView
        android:id="@+id/textView46"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:text="Carga (80%):      "
        android:layout_below="@+id/swCrgSust_repiii"
        android:layout_toLeftOf="@+id/txtCarga_rep_ii_1"
        android:layout_toStartOf="@+id/txtCarga_rep_ii_1" />

    <TextView
        android:id="@+id/textView33"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lecturas:"
        android:layout_below="@+id/ibSelecCarga_rep_ii"
        android:layout_toLeftOf="@+id/textView30"
        android:layout_toStartOf="@+id/textView30"
        android:layout_marginTop="19dp" />

    <TextView
        android:id="@+id/textView34"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lecturas 0 (cero):"
        android:layout_alignBaseline="@+id/textView33"
        android:layout_alignBottom="@+id/textView33"
        android:layout_alignRight="@+id/txtLec1_0"
        android:layout_alignEnd="@+id/txtLec1_0" />

    <EditText
        android:id="@+id/txtCarga_rep_ii_1"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold"
        android:inputType="numberDecimal"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_alignBaseline="@+id/textView46"
        android:layout_alignBottom="@+id/textView46"
        android:layout_alignLeft="@+id/txtCarga_rep_ii_2"
        android:layout_alignStart="@+id/txtCarga_rep_ii_2" />

    <EditText
        android:id="@+id/txtLec1"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:textColor="@color/colorPrimaryDark"
        android:inputType="numberDecimal"
        android:layout_below="@+id/textView34"
        android:layout_alignLeft="@+id/txtLec2"
        android:layout_alignStart="@+id/txtLec2" />

    <EditText
        android:id="@+id/txtLec1_0"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:hint="0"
        android:inputType="numberDecimal"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/txtLec1"
        android:layout_alignBottom="@+id/txtLec1"
        android:layout_toLeftOf="@+id/btGuardar_rep_ii"
        android:layout_toStartOf="@+id/btGuardar_rep_ii" />

    <TextView
        android:id="@+id/textView39"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp"
        android:layout_alignBaseline="@+id/txtLec1"
        android:layout_alignBottom="@+id/txtLec1"
        android:layout_toLeftOf="@+id/txtLec1"
        android:layout_toStartOf="@+id/txtLec1" />

    <EditText
        android:id="@+id/txtLec2"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:textColor="@color/colorPrimaryDark"
        android:inputType="numberDecimal"
        android:layout_below="@+id/txtLec1"
        android:layout_alignLeft="@+id/textView45"
        android:layout_alignStart="@+id/textView45" />

    <EditText
        android:id="@+id/txtLec2_0"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtLec1_0"
        android:layout_alignStart="@+id/txtLec1_0"
        android:layout_below="@+id/txtLec1_0"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:hint="0"
        android:inputType="numberDecimal"
        android:textColor="@color/colorPrimaryDark" />

    <EditText
        android:id="@+id/txtLec3"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:textColor="@color/colorPrimaryDark"
        android:inputType="numberDecimal"
        android:layout_below="@+id/txtLec2"
        android:layout_alignLeft="@+id/txtLec2"
        android:layout_alignStart="@+id/txtLec2" />

    <EditText
        android:id="@+id/txtLec3_0"
        android:layout_width="wrap_content"
        android:textSize="12sp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtLec2_0"
        android:layout_alignStart="@+id/txtLec2_0"
        android:layout_below="@+id/txtLec2_0"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:hint="0"
        android:inputType="numberDecimal"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView40"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtLec2"
        android:layout_alignBottom="@+id/txtLec2"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39"
        android:text="2" />

    <TextView
        android:id="@+id/textView41"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtLec3"
        android:layout_alignBottom="@+id/txtLec3"
        android:layout_alignLeft="@+id/textView40"
        android:layout_alignStart="@+id/textView40"
        android:text="3" />

    <TextView
        android:id="@+id/textView45"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carga:"
        android:layout_alignBaseline="@+id/txtCarga_rep_ii_2"
        android:layout_alignBottom="@+id/txtCarga_rep_ii_2"
        android:layout_alignRight="@+id/textView33"
        android:layout_alignEnd="@+id/textView33" />

    <EditText
        android:id="@+id/txtCarga_rep_ii_2"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:width="80dp"
        android:height="30dp"
        android:textColor="@color/colorAccent"
        android:ems="10"
        android:inputType="numberDecimal"
        android:layout_below="@+id/txtLec3_0"
        android:layout_centerHorizontal="true" />

    <EditText
        android:id="@+id/txtDifMax_rep_ii"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:ems="10"
        android:textColor="@color/colorAccent"
        android:inputType="numberDecimal"
        android:layout_below="@+id/txtCarga_rep_ii_2"
        android:layout_alignLeft="@+id/txtemp_rep_ii"
        android:layout_alignStart="@+id/txtemp_rep_ii" />

    <EditText
        android:id="@+id/txtemp_rep_ii"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="80dp"
        android:height="30dp"
        android:textColor="@color/colorAccent"
        android:ems="10"
        android:inputType="numberDecimal"
        android:layout_below="@+id/txtDifMax_rep_ii"
        android:layout_toRightOf="@+id/txtLec3"
        android:layout_toEndOf="@+id/txtLec3" />

    <TextView
        android:id="@+id/textView47"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dif. Max."
        android:layout_alignBaseline="@+id/txtDifMax_rep_ii"
        android:layout_alignBottom="@+id/txtDifMax_rep_ii"
        android:layout_alignLeft="@+id/textView45"
        android:layout_alignStart="@+id/textView45" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:textSize="12sp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtemp_rep_ii"
        android:layout_alignBottom="@+id/txtemp_rep_ii"
        android:layout_alignLeft="@+id/textView47"
        android:layout_alignStart="@+id/textView47"
        android:text="e.m.p." />

    <TextView
        android:id="@+id/lblResulta_Repii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"
        android:textSize="12sp"
        tools:text="lRes"
        android:layout_alignTop="@+id/btGuardar_rep_ii"
        android:layout_alignLeft="@+id/textView41"
        android:layout_alignStart="@+id/textView41" />

    <Switch
        android:id="@+id/swCrgSust_repiii"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:showText="true"
        android:text="Permitir el ingreso manual de la Carga:  "
        android:textOff="NO"
        android:textOn="SI"
        android:layout_below="@+id/textView30"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39"
        android:layout_alignRight="@+id/ibSelecCarga_rep_ii"
        android:layout_alignEnd="@+id/ibSelecCarga_rep_ii" />

    <Button
        android:id="@+id/btAplicaCrgSust_repiiii"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:text="Aplicar Carga Manual"
        android:textSize="12sp"
        android:layout_above="@+id/ibSelecCarga_rep_ii"
        android:layout_alignRight="@+id/ibSelecCarga_rep_ii"
        android:layout_alignEnd="@+id/ibSelecCarga_rep_ii"
        android:layout_alignLeft="@+id/swCrgSust_repiii"
        android:layout_alignStart="@+id/swCrgSust_repiii" />

    <TextView
        android:id="@+id/lbUniRepiii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="12sp"
        android:textStyle="bold"
        tools:text="lUni"
        android:layout_alignBaseline="@+id/txtCarga_rep_ii_1"
        android:layout_alignBottom="@+id/txtCarga_rep_ii_1"
        android:layout_toRightOf="@+id/txtCarga_rep_ii_1"
        android:layout_toEndOf="@+id/txtCarga_rep_ii_1" />

</RelativeLayout>